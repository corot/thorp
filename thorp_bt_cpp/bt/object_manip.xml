<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="3">
  <include path="manipulation.xml"/>

  <BehaviorTree ID="object_manip">
    <Repeat num_cycles="999999">
      <Sequence>
        <DetectObjects action_name="object_detection"
                       object_types=""
                       server_timeout="5.000000"
                       objects="{objects}"
                       surface="{surface}"/>
        <DragAndDrop action_name="drag_and_drop"
                     objects="{objects}"
                     server_timeout="5.000000"
                     feedback=""
                     object_name="{object_name}"
                     pickup_pose="{pickup_pose}"
                     place_pose="{place_pose}"/>
        <SubTree ID="pickup_object" __autoremap="true"/>
        <SubTree ID="place_object" __autoremap="true"/>
      </Sequence>
    </Repeat>
  </BehaviorTree>
</root>
