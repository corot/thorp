<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="3">
  <include path="manipulation.xml"/>

  <BehaviorTree ID="pickup_objects">
    <KeepRunningUntilFailure>
      <Sequence>
        <DetectObjects action_name="object_detection"
                       object_types=""
                       server_timeout="5.000000"
                       objects="{objects}"
                       surface="{surface}"/>
        <ObjectsDetected objects="{objects}"/>
        <SelectNextTarget failures="{failures}"
                          objects="{objects}"
                          target_name="{object_name}"
                          tightening="{tightening}"/>
        <SubTree ID="pickup_object" __autoremap="true"/>
        <SubTree ID="place_on_tray" __autoremap="true"/>
      </Sequence>
    </KeepRunningUntilFailure>
  </BehaviorTree>
</root>
